{
  "name": "livestore-chess-game-notes",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@livestore/adapter-web": "0.4.0-dev.12",
    "@livestore/livestore": "0.4.0-dev.12",
    "@livestore/peer-deps": "0.4.0-dev.12",
    "@livestore/react": "0.4.0-dev.12",
    "@livestore/sync-cf": "0.4.0-dev.12",
    "@livestore/wa-sqlite": "1.0.5-dev.2",
    "@overengineering/fps-meter": "0.1.2",
    "bcryptjs": "^2.4.3",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.12",
    "@cloudflare/workers-types": "^4.20251011.0",
    "@livestore/devtools-vite": "0.4.0-dev.12",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.8.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "wrangler": "^4.14.4"
  },
  "engines": {
    "node": ">=23.0.0"
  },
  "scripts": {
    "build:dev": "vite build",
    "build": "VITE_LIVESTORE_SYNC_URL=https://chess-game-notes-ws-server.nikolas-burk.workers.dev vite build",
    "dev": "VITE_LIVESTORE_SYNC_URL=http://localhost:8787 vite --force",
    "dev:backend": "wrangler dev --port 8787",
    "dev:frontend": "VITE_LIVESTORE_SYNC_URL=http://localhost:8787 vite --force",
    "dev:all": "concurrently --kill-others --prefix-colors \"cyan,blue\" \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "preview": "vite preview",
    "wrangler:deploy": "wrangler deploy",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types"
  }
}
